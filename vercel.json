{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "style.css", "use": "@vercel/static" },
    { "src": "main.js", "use": "@vercel/static" },
    { "src": "api/socket.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "^/api/socket$", "dest": "/api/socket.js" },
    { "src": "^/$", "dest": "/index.html" },
    { "src": ".*", "dest": "/index.html" }
  ]
}
